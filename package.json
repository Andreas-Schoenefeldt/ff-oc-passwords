{
  "title": "ownCloud Passwords",
  "name": "ff-oc-passwords",
  "version": "0.2.10",
  "description": "A Firefox add-on to access passwords stored in an ownCloud.",
  "homepage": "https://github.com/eglia/ff-oc-passwords",
  "main": "index.js",
  "author": "Andreas Egli",
  "engines": {
    "firefox": ">=30.0",
    "fennec": ">=30.0"
  },
  "license": "MIT",
  "preferences": [{
    "name": "databaseHost",
    "title": "ownCloud URL",
    "type": "string",
    "value": "https://example.org/owncloud",
    "hidden": true
  },
  {
    "name": "refreshTimer",
    "title": "Refresh timer [s]",
    "type": "integer",
    "value": 600,
    "hidden": true
  }]
}
